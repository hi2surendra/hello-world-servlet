pipeline {
  agent any
  stages {
    stage('SourceCode pull') {
      steps {
        echo 'Suren test Checkout Source From Git'
        sh 'sh java -version'
      }
    }
    stage('Build') {
      steps {
        echo 'Suren test Build'
      }
    }
    stage('Build image') {
      steps {
        echo 'Suren test Build Image'
      }
    }
  }
}